  "scripts": {
    "watch": "babel src/ --out-dir dist/ --extensions '.ts,.tsx' --watch",
    "build": "babel src/ --out-dir dist/ --extensions '.ts,.tsx'",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cov": "jest --collectCoverage",
    "prepub": "yarn build",
    "pub": "yarn publish --access public",
    "tsc": "tsc --noEmit",
    "percy:cypress": "percy exec -- cypress run",
    "start:server": "serve -l 8000 .",
    "visual-test": "start-server-and-test start:server 8000 percy:cypress",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280"
  },
